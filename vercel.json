{
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "https://votedeck-production.up.railway.app/:path*"
        },
        {
            "source": "/socket.io/:path*",
            "destination": "https://votedeck-production.up.railway.app/socket.io/:path*"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}